programa
{
	
	funcao inicio()
	{
		inteiro idade

		escreva("Qual é a sua idade: ")
		leia(idade)

		se(idade >= 18 e idade <= 67)
		{
			escreva("Pode doar sangue")
			}

		senao{
			
			escreva("Não pode doar")
			}
	}
}
