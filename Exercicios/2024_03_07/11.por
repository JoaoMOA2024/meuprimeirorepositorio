programa
{
	
	funcao inicio()
	{
		inteiro numeroum
		inteiro numerodois

		escreva("Digite um numero qualquer: ")
		leia(numeroum)

		escreva("Digite um numero qualquer novamente: ")
		leia(numerodois)

		se(numeroum > numerodois)
		{
			escreva("O numero maior é o" + " " + numeroum + " " + "e o numero menor é o" + " " + numerodois)
		
			
			}

		senao
		{
			escreva("O numero maior é o" + " " + numerodois + " " + "e o numero menor é" + " " + numeroum)
		
			
			}
	
	}
}
